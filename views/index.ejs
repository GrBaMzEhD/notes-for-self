<%- include("_header.ejs") %>

    <body>
        <!-- Create a modal for logging in -->
        <div id="loginModal" class="modal">
            <div class="modal-content">
                <span class="close-modal">&times;</span>
                <p>
                    Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                    Fusce lorem sem, volutpat id risus ut, hendrerit pharetra
                    neque. Nam ac urna rhoncus, placerat lectus id, vehicula
                    ligula. Maecenas semper justo est. Phasellus tincidunt
                    pulvinar lectus nec faucibus. Nullam cursus, elit euismod
                    lobortis porttitor, erat nisl semper dui, nec consequat
                    magna mi id enim.
                </p>
            </div>
        </div>
        <div class="sidebar">
            <!-- <span class="about">?</span> -->
            <a class="Logout">
                <img
                    alt="log out button"
                    src="data:image/svg+xml;base64,"
                />
            </a>
        </div>
        <!-- The Boxes on the page layed out in a grid format -->
        <div class="card-container">
            <div class="card" style="width: 18rem">
                <div class="card-body">
                    <h2 class="card-title">Create a New Note</h2>
                    <p class="card-text">
                        Click here to create a new note, if you do not have any
                        notes then click here to get started!
                    </p>
                    <a
                        href="http://127.0.0.1:8000/create-note"
                        class="btn btn-primary"
                        >Create Note</a
                    >
                </div>
            </div>
            <div class="card" style="width: 18rem">
                <div class="card-body">
                    <h2 class="card-title">My Notes</h2>
                    <p class="card-text">
                        Click here to view notes you have created and edit them.
                        If you have no notes then click "Create Note".
                    </p>
                    <a
                        href="http://127.0.0.1:8000/my-notes"
                        class="btn btn-primary"
                        >My Notes</a
                    >
                </div>
            </div>
            <div class="card" style="width: 18rem">
                <div class="card-body">
                    <h2 class="card-title">View Pinboard</h2>
                    <p class="card-text">
                        Here you can link notes together using pins and create
                        whole boards of notes.
                    </p>
                    <a
                        href="http://127.0.0.1:8000/create-note"
                        class="btn btn-primary"
                        >View Pinboard</a
                    >
                </div>
            </div>
        </div>
        <%- include("_footer.ejs") %>
    </body>
</html>
